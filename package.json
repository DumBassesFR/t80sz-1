    "url": "https://github.com/ryeenii/t80sz/issues"
  },
  "homepage": "https://the80s.zone",
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "autoprefixer": "^9",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "nodemon": "^2.0.7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "3.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "latest",
